let fegBscAvail = fegEthAvail = 0;
let bnbPrice = ethPrice = bnbFegPrice = ethFegPrice = 0;
function init(){
    getPricesBase();
}

function getPricesBase(){
    $.ajax({
        type: "GET",
        url: "https://api.coingecko.com/api/v3/simple/price?ids=ethereum%2Cbinancecoin&vs_currencies=usd",
        success: function(datas){
            bnbPrice = datas.binancecoin.usd;
            ethPrice = datas.ethereum.usd;
            getBurnt(getCombinedMC);
        }
    });
  }

function getBurnt(callback){
      contractEthFeg.balanceOf('0x000000000000000000000000000000000000dead').then(ret => {
          fegb =  ret / Math.pow(10,24);
          fegb = Math.round(fegb * 100000) / 100000
          $('.fegBurnt').html(fegb+" Q");
          fegEthAvail = 100000000000000000000000000 - ret;
          $('.fegAvail').html(100 - fegb+" Q");
          //getPricesFegEth();
          contractUniswapEthFeg.getReserves().then(reserves => {
              ethFegPrice = (Number(reserves._reserve0) / Number(reserves._reserve1) * 1e9).toFixed(0);
              let price = parseFloat(ethPrice)/parseFloat(ethFegPrice);
              $('#fegPrice').text(price.toFixed(15)+" $US")
              $('#fegMc').text(Number(((price * fegEthAvail) / 1e9).toFixed(0)).toLocaleString()+ " $US")
              $fegETHmcVar = (price * fegEthAvail) / 1e9
              callback($fegETHmcVar);
          })
      });
        contractBscFeg.balanceOf('0x000000000000000000000000000000000000dead').then(ret => {
        fegb =  ret / Math.pow(10,24);
        fegb = Math.round(fegb * 100000) / 100000
        $('.fegBscBurnt').html(fegb+" Q");
        fegBscAvail = 100000000000000000000000000 - ret;
        $('.fegBscAvail').html(100 - fegb+" Q");
        //getPricesFegBsc();
        contractPancakeBnbFeg.getReserves().then(reserves => {
            bnbFegPrice = (Number(reserves._reserve0) / Number(reserves._reserve1) * 1e9).toFixed(0);
            let price = parseFloat(bnbPrice)/parseFloat(bnbFegPrice);
            $('#fegBscPrice').text(price.toFixed(15)+" $US");
            $('#fegBscMc').text(Number(((price * fegBscAvail) / 1e9).toFixed(0)).toLocaleString()+ " $US")
            $fegBSCmcVar = (price * fegBscAvail) / 1e9
            callback($fegBSCmcVar);
        })
      });
}

    function getCombinedMC(){
        $('#fegCombinedMc').text(Number(($fegBSCmcVar + $fegETHmcVar).toFixed(0)).toLocaleString()+ " $US")
    }
init();

